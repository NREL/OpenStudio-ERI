  <class_name>ProcessConstructionsInteriorUninsulatedFloors</class_name>
  <display_name>ProcessConstructionsInteriorUninsulatedFloors</display_name>
  <description>This measure creates uninsulated constructions for the floors between living spaces and the floors between the living space and finished basement.</description>
  <modeler_description>Calculates material layer properties of uninsulated constructions for the floors between living spaces and the floors between the living space and finished basement. Finds surfaces adjacent to the living space and finished basement and sets applicable constructions.</modeler_description>
  <arguments>
    <argument>
      <name>selectedliving</name>
      <display_name>Of what space type is the living space?</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>selectedfbsmt</name>
      <display_name>Of what space type is the finished basement?</display_name>
      <type>Choice</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>userdefinedgypthickness</name>
      <display_name>Thickness of drywall layers [in].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.5</default_value>
    </argument>
    <argument>
      <name>userdefinedgyplayers</name>
      <display_name>Number of drywall layers.</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
    <argument>
      <name>userdefinedfloormassth</name>
      <display_name>Floor mass thickness [in].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.625</default_value>
    </argument>
    <argument>
      <name>userdefinedfloormasscond</name>
      <display_name>Floor mass conductivity [Btu-in/h-ft^2-R].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.8004</default_value>
    </argument>
    <argument>
      <name>userdefinedfloormassdens</name>
      <display_name>Floor mass density [lb/ft^3].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>34</default_value>
    </argument>
    <argument>
      <name>userdefinedfloormasssh</name>
      <display_name>Floor mass specific heat [Btu/lb-R].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.29</default_value>
    </argument>
    <argument>
      <name>userdefinedcarpetr</name>
      <display_name>Carpet pad R-value [hr-ft^2-R/Btu].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>2.08</default_value>
    </argument>
    <argument>
      <name>userdefinedcarpetfrac</name>
      <display_name>Carpet floor fraction [frac].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.8</default_value>
    </argument>
  </arguments>
