  <class_name>ProcessConstructionsSlab</class_name>
  <display_name>ProcessConstructionsSlab</display_name>
  <description>This measure creates slab constructions for the living space floor.</description>
  <modeler_description>Calculates material layer properties of slab constructions for the living space floor. Finds surfaces adjacent to the living space and sets applicable constructions.</modeler_description>
  <arguments>
    <argument>
      <name>selectedliving</name>
      <display_name>Of what space type is the living space?</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>selectedslabins</name>
      <display_name>Slab insulation type.</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <choices>
        <choice>
          <value>Uninsulated</value>
          <display_name>Uninsulated</display_name>
        </choice>
        <choice>
          <value>Perimeter</value>
          <display_name>Perimeter</display_name>
        </choice>
        <choice>
          <value>Exterior</value>
          <display_name>Exterior</display_name>
        </choice>
        <choice>
          <value>Whole Slab</value>
          <display_name>Whole Slab</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>userdefinedslabperiextr</name>
      <display_name>Slab perimeter insulation R-value or exterior insulation R-value [hr-ft^2-R/Btu].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>userdefinedslabperiextwidthdepth</name>
      <display_name>Slab perimeter insulation width or exterior insulation depth [ft].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>userdefinedslabgapr</name>
      <display_name>Perimeter gap R-value or whole slab gap R-value [hr-ft^2-R/Btu].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>userdefinedslabwholer</name>
      <display_name>Whole slab insulation R-value [hr-ft^2-R/Btu].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>userdefinedcarpetr</name>
      <display_name>Carpet pad R-value [hr-ft^2-R/Btu].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>2.08</default_value>
    </argument>
    <argument>
      <name>userdefinedcarpetfrac</name>
      <display_name>Carpet floor fraction [frac].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.8</default_value>
    </argument>
    <argument>
      <name>userdefinedslabarea</name>
      <display_name>Area of the slab foundation [ft^2].</display_name>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1200</default_value>
    </argument>
    <argument>
      <name>userdefinedslabextperim</name>
      <display_name>Perimeter of the slab foundation [ft].</display_name>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>140</default_value>
    </argument>
  </arguments>
