      <name>userdefinedpartitionwallmassth</name>
      <display_name>Partition Wall Mass: Thickness</display_name>
      <description>Thickness of the layer.</description>
      <type>Double</type>
      <units>in</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.5</default_value>
    </argument>
    <argument>
      <name>userdefinedpartitionwallmasscond</name>
      <display_name>Partition Wall Mass: Conductivity</display_name>
      <description>Conductivity of the layer.</description>
      <type>Double</type>
      <units>Btu-in/h-ft^2-R</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>1.1112</default_value>
    </argument>
    <argument>
      <name>userdefinedpartitionwallmassdens</name>
      <display_name>Partition Wall Mass: Density</display_name>
      <description>Density of the layer.</description>
