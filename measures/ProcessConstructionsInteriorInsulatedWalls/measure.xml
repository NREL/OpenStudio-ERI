  <class_name>ProcessConstructionsInteriorInsulatedWalls</class_name>
  <display_name>Add/Replace Residential Interzonal Walls</display_name>
  <description>This measure creates insulated constructions for the interzonal walls between the living space and the garage.</description>
  <modeler_description>Calculates material layer properties of insulated constructions for the interzonal walls between the living space and the garage. Finds surfaces adjacent to the living space and garage and sets applicable constructions.</modeler_description>
  <arguments>
    <argument>
      <name>selectedliving</name>
      <display_name>Living Space</display_name>
      <description>The living space type.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>selectedgarage</name>
      <display_name>Garage Space</display_name>
      <description>The garage space type.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>userdefinedpartitionwallmassth</name>
      <display_name>Partition Wall Mass: Thickness</display_name>
      <description>Thickness of the layer.</description>
      <type>Double</type>
      <units>in</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.5</default_value>
    </argument>
    <argument>
      <name>userdefinedpartitionwallmasscond</name>
      <display_name>Partition Wall Mass: Conductivity</display_name>
      <description>Conductivity of the layer.</description>
      <type>Double</type>
      <units>Btu-in/h-ft^2-R</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>1.1112</default_value>
    </argument>
    <argument>
      <name>userdefinedpartitionwallmassdens</name>
      <display_name>Partition Wall Mass: Density</display_name>
      <description>Density of the layer.</description>
      <type>Double</type>
      <units>lb/ft^3</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>50</default_value>
    </argument>
    <argument>
      <name>userdefinedpartitionwallmasssh</name>
      <display_name>Partition Wall Mass: Specific Heat</display_name>
      <description>Specific heat of the layer.</description>
      <type>Double</type>
      <units>Btu/lb-R</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.2</default_value>
    </argument>
    <argument>
      <name>selectedstudsize</name>
      <display_name>Interzonal Walls: Cavity Depth</display_name>
      <description>Depth of the stud cavity.</description>
      <type>Choice</type>
      <units>in</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>2x4</default_value>
      <choices>
        <choice>
          <value>2x4</value>
          <display_name>2x4</display_name>
        </choice>
        <choice>
          <value>2x6</value>
          <display_name>2x6</display_name>
        </choice>
        <choice>
          <value>2x8</value>
          <display_name>2x8</display_name>
        </choice>
        <choice>
          <value>2x10</value>
          <display_name>2x10</display_name>
        </choice>
        <choice>
          <value>2x12</value>
          <display_name>2x12</display_name>
        </choice>
        <choice>
          <value>2x14</value>
          <display_name>2x14</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>selectedspacing</name>
      <display_name>Interzonal Walls: Stud Spacing</display_name>
      <description>The on-center spacing between studs in a wall assembly.</description>
      <type>Choice</type>
      <units>in</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>16 in o.c.</default_value>
      <choices>
        <choice>
          <value>16 in o.c.</value>
          <display_name>16 in o.c.</display_name>
        </choice>
        <choice>
          <value>24 in o.c.</value>
          <display_name>24 in o.c.</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>userdefinedinstcavr</name>
      <display_name>Interzonal Walls: Cavity Insulation Installed R-value</display_name>
      <description>R-value is a measure of insulation's ability to resist heat traveling through it.</description>
      <type>Double</type>
      <units>hr-ft^2-R/Btu</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>13</default_value>
    </argument>
    <argument>
      <name>selectedinstallgrade</name>
      <display_name>Interzonal Walls: Cavity Install Grade</display_name>
      <description>5% of the wall is considered missing insulation for Grade 3, 2% for Grade 2, and 0% for Grade 1.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>I</default_value>
      <choices>
        <choice>
          <value>I</value>
          <display_name>I</display_name>
        </choice>
        <choice>
          <value>II</value>
          <display_name>II</display_name>
        </choice>
        <choice>
          <value>III</value>
          <display_name>III</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>selectedinsfills</name>
      <display_name>Interzonal Walls: Insulation Fills Cavity</display_name>
      <description>Specifies whether the cavity insulation completely fills the depth of the wall cavity.</description>
      <type>Boolean</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>true</default_value>
      <choices>
        <choice>
          <value>true</value>
          <display_name>true</display_name>
        </choice>
        <choice>
          <value>false</value>
          <display_name>false</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>userdefinedrigidinsthickness</name>
      <display_name>Interzonal Walls: Continuous Insulation Thickness</display_name>
      <description>The thickness of the continuous insulation.</description>
      <type>Double</type>
      <units>in</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0</default_value>
    </argument>
    <argument>
      <name>userdefinedrigidinsr</name>
      <display_name>Interzonal Walls: Continuous Insulation Nominal R-value</display_name>
      <description>R-value is a measure of insulation's ability to resist heat traveling through it.</description>
      <type>Double</type>
      <units>hr-ft^2-R/Btu</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0</default_value>
    </argument>
  </arguments>
