  <class_name>ResidentialDishwasher</class_name>
  <display_name>ResidentialDishwasher</display_name>
  <description>Adds a dishwasher to the user selected space. The energy consumption can either be defined based on the Building America Benchmark dishwasher (which consumes 87.6 + 29.2 * Num_bedrooms kWh/yr and 2.26 + 0.75 * Num_bedrooms gal/day) or a user defined annual energy and hot water consumption. </description>
  <modeler_description>Adds an electric equipment object. TODO: SHould this add a hot water consumption object (once water heaters are added), or should the water heater measure have objects for has_dishwasher, etc.</modeler_description>
  <arguments>
    <argument>
      <name>selected_dw</name>
      <display_name>Dishwasher Energy Consumption Option</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <choices>
        <choice>
          <value>Benchmark</value>
          <display_name>Benchmark</display_name>
        </choice>
        <choice>
          <value>Simple</value>
          <display_name>Simple</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>dw_E</name>
      <display_name>Simple Dishwasher Annual Energy Consumption (kWh/yr)</display_name>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0</default_value>
    </argument>
    <argument>
      <name>BAB_mult</name>
      <display_name>Building America Benchmark Multipler</display_name>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
    <argument>
      <name>Num_Br</name>
      <display_name>Number of Bedrooms</display_name>
      <type>Integer</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
    <argument>
      <name>space_type</name>
      <display_name>Select the space where the dishwasher is located</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
  </arguments>
