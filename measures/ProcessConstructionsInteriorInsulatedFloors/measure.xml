  <class_name>ProcessConstructionsInteriorInsulatedFloors</class_name>
  <display_name>ProcessConstructionsInteriorInsulatedFloors</display_name>
  <description>This measure creates insulated constructions for the interzonal floors between the living space and the garage.</description>
  <modeler_description>Calculates material layer properties of insulated constructions for the interzonal floors between the living space and the garage. Finds surfaces adjacent to the living space and garage and sets applicable constructions.</modeler_description>
  <arguments>
    <argument>
      <name>selectedliving</name>
      <display_name>Of what space type is the living space?</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>selectedgarage</name>
      <display_name>Of what space type is the garage?</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>userdefinedinstcavr</name>
      <display_name>R-value of cavity insulation [hr-ft^2-R/Btu].</display_name>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>userdefinedfloorff</name>
      <display_name>Interzonal floor framing factor [frac].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.13</default_value>
    </argument>
    <argument>
      <name>userdefinedfloormassth</name>
      <display_name>Floor mass thickness [in].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.625</default_value>
    </argument>
    <argument>
      <name>userdefinedfloormasscond</name>
      <display_name>Floor mass conductivity [Btu-in/h-ft^2-R].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.8004</default_value>
    </argument>
    <argument>
      <name>userdefinedfloormassdens</name>
      <display_name>Floor mass density [lb/ft^3].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>34</default_value>
    </argument>
    <argument>
      <name>userdefinedfloormasssh</name>
      <display_name>Floor mass specific heat [Btu/lb-R].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.29</default_value>
    </argument>
    <argument>
      <name>userdefinedcarpetr</name>
      <display_name>Carpet pad R-value [hr-ft^2-R/Btu].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>2.08</default_value>
    </argument>
    <argument>
      <name>userdefinedcarpetfrac</name>
      <display_name>Carpet floor fraction [frac].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.8</default_value>
    </argument>
  </arguments>
