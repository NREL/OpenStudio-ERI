      <type>Integer</type>
      <units>#</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>2</default_value>
    </argument>
    <argument>
      <name>inset_width</name>
      <display_name>Inset Width</display_name>
