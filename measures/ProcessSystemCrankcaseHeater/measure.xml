  <class_name>ProcessSystemCrankcaseHeater</class_name>
  <display_name>Set Residential Crankcase Heater for Heat Pump and Multispeed Air Conditioner</display_name>
  <description>This measure creates a crankcase heater for heat pumps and multispeed air conditioners.</description>
  <modeler_description>Using EMS code, this measure creates a crankcase heater electric equipment object for heat pumps and also air conditioners with number of compressor speeds greater than one.</modeler_description>
  <arguments>
    <argument>
      <name>userdefinedcrankcase</name>
      <display_name>Crankcase [kW]</display_name>
      <description>Capacity of the crankcase heater for the compressor.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0.02</default_value>
    </argument>
    <argument>
      <name>userdefinedcrankcasemaxt</name>
      <display_name>Crankcase Max Temp [degrees F]</display_name>
      <description>Outdoor dry-bulb temperature above which compressor crankcase heating is disabled.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>55</default_value>
    </argument>
    <argument>
      <name>living_thermal_zone</name>
      <display_name>Living thermal zone</display_name>
      <description>Select the living thermal zone</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>living zone</default_value>
      <choices>
        <choice>
          <value>living zone</value>
          <display_name>living zone</display_name>
        </choice>
      </choices>
    </argument>
  </arguments>
