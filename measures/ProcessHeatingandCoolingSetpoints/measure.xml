      <name>selectedliving</name>
      <display_name>Select the living zone.</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>userdefinedhsp</name>
      <display_name>Enter the constant heating setpoint [F].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>71</default_value>
    </argument>
    <argument>
