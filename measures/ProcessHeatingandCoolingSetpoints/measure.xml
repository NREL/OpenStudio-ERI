      <display_name>Enter the constant cooling setpoint [F].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>76</default_value>
    </argument>
    <argument>
      <name>selectedheating</name>
      <display_name>The house has heating equipment.</display_name>
      <type>Boolean</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>true</default_value>
      <choices>
