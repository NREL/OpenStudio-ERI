  <class_name>ProcessThermalMassPartitionWall</class_name>
  <display_name>Add/Replace Residential Partition Wall Thermal Mass</display_name>
  <description>This measure creates internal mass for partition walls in the living space and finished basement.</description>
  <modeler_description>This measure creates constructions representing the internal mass of partition walls in the living space and finished basement. The constructions are set to define the internal mass objects of their respective spaces.</modeler_description>
  <arguments>
    <argument>
      <name>partitionwallmassth</name>
      <display_name>Partition Wall Mass: Thickness</display_name>
      <description>Thickness of the layer.</description>
      <type>Double</type>
      <units>in</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.5</default_value>
    </argument>
    <argument>
      <name>partitionwallmasscond</name>
      <display_name>Partition Wall Mass: Conductivity</display_name>
      <description>Conductivity of the layer.</description>
      <type>Double</type>
      <units>Btu-in/h-ft^2-R</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>1.1112</default_value>
    </argument>
    <argument>
      <name>partitionwallmassdens</name>
      <display_name>Partition Wall Mass: Density</display_name>
      <description>Density of the layer.</description>
      <type>Double</type>
      <units>lb/ft^3</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>50</default_value>
    </argument>
    <argument>
      <name>partitionwallmasssh</name>
      <display_name>Partition Wall Mass: Specific Heat</display_name>
      <description>Specific heat of the layer.</description>
      <type>Double</type>
      <units>Btu/lb-R</units>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.2</default_value>
    </argument>
    <argument>
      <name>partitionwallfrac</name>
      <display_name>Partition Wall Mass: Fraction of Floor Area</display_name>
      <description>Ratio of exposed partition wall area to total conditioned floor area and accounts for the area of both sides of partition walls.</description>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
    <argument>
      <name>living_space_type</name>
      <display_name>Living space type</display_name>
      <description>Select the living space type</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>living</default_value>
      <choices>
        <choice>
          <value>living</value>
          <display_name>living</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>fbasement_space_type</name>
      <display_name>Finished basement space type</display_name>
      <description>Select the finished basement space type</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>finished basement</default_value>
      <choices>
        <choice>
          <value>finished basement</value>
          <display_name>finished basement</display_name>
        </choice>
      </choices>
    </argument>
  </arguments>
